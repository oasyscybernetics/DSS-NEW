<script type="text/javascript" src="/library/download/3?preview=1"></script><script type="text/javascript" src="/library/download/4?preview=1"></script>
<script type="text/javascript">
    function updateClock(){
        var now = moment().add(0, "m"),
                second = now.seconds() * 6,
                minute = now.minutes() * 6 + second / 60,
                hour = ((now.hours() % 12) / 12) * 360 + 90 + minute / 12;

        $('#hour').css("transform", "rotate(" + hour + "deg)");
        $('#minute').css("transform", "rotate(" + minute + "deg)");
        $('#second').css("transform", "rotate(" + second + "deg)");
    }

    $(document).ready(function() {

        var options = {"originalWidth":"1920","originalHeight":"1080","previewWidth":0,"previewHeight":0};
        var previewWidth = options.previewWidth;
        var previewHeight = options.previewHeight;

        if (previewWidth == 0 || previewHeight == 0)
            var size = Math.min($(window).width(), $(window).height());
        else
            var size = Math.min(previewWidth, previewHeight);

        // Shrink by 2px to get a bit of space to the side.
        size -= 2;

        $(".hero-circle").css({
            width: size + "px",
            height: size + "px"
        });

        updateClock();
        setInterval(updateClock, 1000);
    });
</script>
</html>

